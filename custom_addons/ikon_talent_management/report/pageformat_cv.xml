<odoo>
    <record id="paperformat_applicant" model="report.paperformat">
        <field name="name">CV Employee</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">16</field>
        <field name="margin_bottom">16</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">16</field>
        <field name="dpi">90</field>
        <field name="disable_shrinking" eval="False"/>
    </record>
</odoo>