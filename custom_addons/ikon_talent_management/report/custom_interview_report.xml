<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Print First Interview Report -->
    <record id="action_print_interview_one_report" model="ir.actions.report">
        <field name="name">Screening Interview</field>
        <field name="model">hr.applicant</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">ikon_talent_management.custom_interview_one_report</field>
        <field name="report_file">ikon_talent_management.custom_interview_one_report</field>
        <field name="print_report_name">'Screening Information - %s' % (object.partner_name)</field>
        <field name="binding_model_id" ref="model_hr_applicant"/>
       <field name="binding_type">report</field>
    </record>

    <!-- Print Second Interview Report -->
    <record id="action_print_interview_two_report" model="ir.actions.report">
        <field name="name">CBI Interview</field>
        <field name="model">hr.applicant</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">ikon_talent_management.custom_interview_two_report</field>
        <field name="report_file">ikon_talent_management.custom_interview_two_report</field>
        <field name="print_report_name">'CBI Information - %s' % (object.partner_name)</field>
        <field name="binding_model_id" ref="model_hr_applicant"/>
       <field name="binding_type">report</field>
    </record>
</odoo>