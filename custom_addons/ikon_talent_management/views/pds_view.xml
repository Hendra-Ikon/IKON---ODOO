<odoo>
    <data>
        <template id="custom_pds_data">
            <t t-call="website.layout">
                <ul>
                    <t t-foreach="data" t-as="record">
                        <li>
                            <t t-esc="record.fullname"/>
                        </li>
                        <li>
                            <t t-esc="record.nik"/>
                        </li>
                        <li>
                            <t t-esc="record.addressNIK"/>
                        </li>
                        <!-- Add more fields as needed -->
                    </t>
                </ul>
            </t>

        </template>
    </data>
</odoo>